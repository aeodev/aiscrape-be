PORT=3001
NODE_ENV=development
MONGODB_URI=mongodb://localhost:27017/aiscrape
CLIENT_URL=http://localhost:5173
GEMINI_API_KEY=your-gemini-api-key-here

# Scraping Configuration
MAX_CONCURRENT_SCRAPES=5
SCRAPE_TIMEOUT=60000
USER_AGENT=Mozilla/5.0 (compatible; AIScrape/1.0)

# Performance
BLOCK_RESOURCES=true
REUSE_BROWSER=true
SCREENSHOT_ENABLED=true
SCREENSHOT_FULL_PAGE=false
SCREENSHOT_STORAGE_PATH=uploads/screenshots

# Anti-Bot
ROTATE_USER_AGENTS=true
REQUEST_DELAY_MIN=1000
REQUEST_DELAY_MAX=3000

# Resilience
MAX_RETRIES=3
RETRY_BACKOFF_BASE=1000
