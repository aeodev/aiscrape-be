    "@types/natural": "^5.1.5",
    "@types/node": "^20.10.6",
    "@types/opossum": "^8.1.9",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^2.0.16",
    "@types/turndown": "^5.0.6",
    "jest": "^29.7.0",
    "puppeteer": "^24.32.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.6",
