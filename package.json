    "crawlee": "^3.7.2",
    "domhandler": "^5.0.3",
    "domutils": "^3.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsdom": "^27.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "natural": "^8.1.0",
    "opossum": "^7.1.0",
    "playwright": "^1.40.1",
    "playwright-extra": "^4.3.6",
