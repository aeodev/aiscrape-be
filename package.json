{
  "name": "aiscrape-backend",
  "version": "1.0.0",
  "description": "AI-powered web scraping backend with Crawlee",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
